#include "imgproc/xf_channel_extract.hpp"  
#include "xfSplitCoreForVivadoHLS.h"   
 
void xF_split(	
	xf::Mat<${srcTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit}, ${maxWidthCMakeParamSplit}, ${NPCCMakeParamSplit}>* imgInput,		
	xf::Mat<${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit}, ${maxWidthCMakeParamSplit}, ${NPCCMakeParamSplit}>* imgOutput0,
	xf::Mat<${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit}, ${maxWidthCMakeParamSplit}, ${NPCCMakeParamSplit}>* imgOutput1,
	xf::Mat<${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit}, ${maxWidthCMakeParamSplit}, ${NPCCMakeParamSplit}>* imgOutput2,
	xf::Mat<${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit}, ${maxWidthCMakeParamSplit}, ${NPCCMakeParamSplit}>* imgOutput3)
{ 
 
	    
	// call xf::extractChannel 
	xf::extractChannel<${srcTypeCMakeParamSplit},${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit},${maxWidthCMakeParamSplit},${NPCCMakeParamSplit}>(*imgInput, *imgOutput0, XF_EXTRACT_CH_R);	
	xf::extractChannel<${srcTypeCMakeParamSplit},${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit},${maxWidthCMakeParamSplit},${NPCCMakeParamSplit}>(*imgInput, *imgOutput1, XF_EXTRACT_CH_G);	
	xf::extractChannel<${srcTypeCMakeParamSplit},${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit},${maxWidthCMakeParamSplit},${NPCCMakeParamSplit}>(*imgInput, *imgOutput2, XF_EXTRACT_CH_B);	
	xf::extractChannel<${srcTypeCMakeParamSplit},${dstTypeCMakeParamSplit}, ${maxHeightCMakeParamSplit},${maxWidthCMakeParamSplit},${NPCCMakeParamSplit}>(*imgInput, *imgOutput3, XF_EXTRACT_CH_A);
	 
	return;
}

